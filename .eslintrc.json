{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "spaced-comment": [2, "never"],
        "no-restricted-globals": ["error", "event", "fdescribe"]
    }
}
